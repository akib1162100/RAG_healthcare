<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_prescription_order" model="ir.sequence">
            <field name="name">Prescription Order</field>
            <field name="code">prescription.order.knk</field>
            <field name="prefix">PRE</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>
        <record id="sequence_patient_id" model="ir.sequence">
            <field name="name">patient id</field>
            <field name="code">patient.id.knk</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="name">Employee Record Sequence</field>
        </record>
        <record id="sequence_appointment_id" model="ir.sequence">
            <field name="name">Appointment id</field>
            <field name="code">appointment.id.knk</field>
            <field name="prefix">APT/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>
    </data>
    <record id="radiology_request_seq" model="ir.sequence">
        <field name="name">Radiology Request</field>
        <field name="code">radiology.request</field>
        <field name="prefix">RR/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="custom_invoice_seq" model="ir.sequence">
        <field name="name">Custom Invoice</field>
        <field name="code">custom.invoice</field>
        <field name="prefix">INV/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>
</odoo>