<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pos_prescription_complaint_view_form" model="ir.ui.view">
        <field name="name">Complaint.list.view.form</field>
        <field name="model">complaint.list</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="period_ids" widget="many2many_tags"/>
                            <field name="company_id" />
							
							<field name="trigger_context_ids"
								   widget="many2many_tags"
								   options="{'no_create': False}"
								  />
							<field name="temporal_pattern_ids"
								   widget="many2many_tags"
								   options="{'no_create': False}"
								   />
                            <field name="allergy_reaction_ids"
                                   widget="many2many_tags"
                                   options="{'no_create': False}"/>
                            <field name="red_flag_ids"
                                   widget="many2many_tags"
                                   options="{'no_create': False}"/>

							<field name="clinical_overview"/>
                        </group>
                    </group>

                    <notebook>
                        <page string="Images">
                            <field name="complain_image_line_ids">
                                <tree editable="bottom">
                                    <field name="name" />
                                    <field name="image" widget="image" options="{'zoom': true, 'size': [100, 100]}"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_complaint_image_select_wizard_form" model="ir.ui.view">
        <field name="name">complaint.image.select.wizard.form</field>
        <field name="model">complaint.image.select.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="complaint_id" readonly="1" force_save="1"/>
                </group>
                <field name="image_line_ids">
                    <tree create="0" delete="0" editable="bottom">
                        <field name="is_selected"/>
                        <field name="image_id" readonly="1" force_save="1"/>
                        <field name="image" widget="image" readonly="1" force_save="1" options="{'zoom': true, 'size': [50, 50]}"/>
                    </tree>
                </field>
                <footer>
                    <button name="action_confirm" type="object" string="Confirm" class="btn-success"
                            confirm="Are you sure want to confirm?"/>
                    <button string="Cancel" class="btn-danger" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


