<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="res_prescription_order_form_view" model="ir.ui.view">
            <field name="name">prescription.order.knk.form</field>
            <field name="model">prescription.order.knk</field>
            <field name="inherit_id" ref="pos_prescription_knk.view_hms_prescription_order_form"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='patient_seq']" position="after">
                    <field name="wk_appointment_id" attrs="{'readonly': True}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>